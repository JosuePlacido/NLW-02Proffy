{
  "expo": {
    "name": "Proffy",
    "slug": "Proffy",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/icon.png",
    "splash": {
      "image": "./src/assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#8257E5"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
	  "supportsTablet": true,
	  "bundleIdentifier":"com.alternativadigital.proffy"
    },
    "web": {
      "favicon": "./assets/favicon.png"
	},
	"android":{
		"package":"com.alternativadigital.proffy"
	}
  }
}
